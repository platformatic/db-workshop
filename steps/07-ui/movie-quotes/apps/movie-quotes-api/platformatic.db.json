{
  "server": {
    "logger": {
      "level": "info"
    },
    "hostname": "127.0.0.1",
    "port": "3042",
    "cors": {
      "origin": "http://127.0.0.1:3000"
    }
  },
  "db": {
    "connectionString": "sqlite://db.sqlite",
    "graphiql": true,
    "openapi": true,
